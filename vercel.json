{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/save-profile", "dest": "server.js" },
      { "src": "/profiles/.*", "dest": "server.js" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }